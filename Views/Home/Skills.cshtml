@{
    ViewData["Title"] = "Skills";
}

<section class="skills" id="skills">
    <h2 class="skill-header">My Top Skills</h2>

    <div class="skills-wrapper">
        <div class="first-set animate__animated animate__pulse">
            <img src="~/images/icons8-.net-framework.svg" alt="" loading="lazy" class="icon icon-card" />
            <img src="~/images/icons8-spring-boot.svg" alt="" loading="lazy" class="icon icon-card" />
            <img src="~/images/icons8-python.svg" alt="" loading="lazy" class="icon icon-card" />
        </div>

        <div class="second-set animate__animated animate__pulse">
            <img src="~/images/icons8-c++.svg" alt="" loading="lazy" class="icon icon-card" />
            <img src="~/images/icons8-java.svg" alt="" loading="lazy" class="icon icon-card" />
            <img src="~/images/icons8-git.svg" alt="" loading="lazy" class="icon icon-card" />
        </div>
    </div>
</section>